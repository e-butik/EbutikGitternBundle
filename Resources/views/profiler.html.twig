{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="22" height="22" alt="Gittern" style="border-width: 0; vertical-align: middle; margin-right: 5px;" src="data:image/png;base64,"/>
    {% endset %}
    {% set text %}
        <span>{{collector.rawObjectFetches|length}}</span>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block head %}
    {{parent()}}
    {# if the web profiler panel needs some specific JS or CSS files #}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img src="{{ asset('bundles/ebutikgittern/images/profiler/git-small.png') }}" style="width: 34px; height:34px;" alt="Gittern Transport" /></span>
    <strong>Gittern</strong>
    <span class="count">
        <span>{{collector.rawObjectFetches|length}}</span>
        <span>{{collector.totalRawObjectFetchTime|number_format(0)}} ms</span>
    </span>
</span>
{% endblock %}

{% block panel %}
    <h2>Raw Object Fetches</h2>

    <p><strong>Total time spent fetching raw objects:</strong> {{collector.totalRawObjectFetchTime|number_format(2)}} ms</p>

    <table>
        <tr>
            <th>SHA</th>
            <th>Type</th>
            <th>Time took (ms)</th>
        </tr>
        {% for fetch in collector.rawObjectFetches %}
        <tr>
            <td>{{fetch.sha}}</td>
            <td>{{fetch.type}}</td>
            <td>{{fetch.time|number_format(2)}}</td>
        </tr>
        {% endfor %}
    </table>

    <h2>Raw object puts</h2>
    <p><strong>Total time spent putting raw objects:</strong> {{collector.totalRawObjectPutTime|number_format(2)}} ms</p>

    <table>
        <tr>
            <th>SHA</th>
            <th>Type</th>
            <th>Length</th>
            <th>Time took (ms)</th>
        </tr>
        {% for put in collector.rawObjectPuts %}
        <tr>
            <td>{{put.sha}}</td>
            <td>{{put.type}}</td>
            <td>{{put.length}}</td>
            <td>{{put.time|number_format(2)}}</td>
        </tr>
        {% endfor %}
    </table>

    <h2>Treeish resolves</h2>
    <p><strong>Total time spent resolving treeishes:</strong> {{collector.totalTreeishResolutionTime|number_format(2)}} ms</p>

    <table>
        <tr>
            <th>Treeish</th>
            <th>Time took (ms)</th>
        </tr>
        {% for resolve in collector.treeishResolutions %}
        <tr>
            <td>{{resolve.treeish}}</td>
            <td>{{resolve.time|number_format(2)}}</td>
        </tr>
        {% endfor %}
    </table>

    <h2>Index data puts</h2>

    <table>
        <tr>
            <th>Length</th>
            <th>Time took (ms)</th>
        </tr>
        {% for put in collector.indexDataPuts %}
        <tr>
            <td>{{put.length}}</td>
            <td>{{put.time|number_format(2)}}</td>
        </tr>
        {% endfor %}
    </table>

    <h2>Index data gets</h2>

    <table>
        <tr>
            <th>Length</th>
            <th>Time took (ms)</th>
        </tr>
        {% for get in collector.indexDataGets %}
        <tr>
            <td>{{get.length}}</td>
            <td>{{get.time|number_format(2)}}</td>
        </tr>
        {% endfor %}
    </table>
{% endblock %}